{
  "name": "zisui",
  "version": "1.3.3",
  "description": "Fast and simple storybook screenshooter.",
  "bin": {
    "zisui": "lib/node/cli.js"
  },
  "typings": "lib/client/index.d.ts",
  "main": "lib/client/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*\" \"scripts/**/*\"",
    "format:check": "prettier --check \"src/**/*\" \"scripts/**/*\"",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "clihelp": "node scripts/cli-help.js",
    "prepare": "yarn clean && yarn build && yarn clihelp",
    "test": "sh e2e.sh",
    "regression": "reg-suit run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quramy/zisui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "screenshot"
  ],
  "author": "quramy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quramy/zisui/issues"
  },
  "homepage": "https://github.com/Quramy/zisui#readme",
  "devDependencies": {
    "@types/react": "^16.4.9",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "copy-dir": "^1.1.0",
    "cpy": "^7.3.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "husky": "^3.0.2",
    "minimist": "^1.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.4.2",
    "reg-keygen-git-hash-plugin": "^0.7.16",
    "reg-notify-github-plugin": "^0.7.21",
    "reg-publish-s3-plugin": "^0.7.22",
    "reg-suit": "^0.7.17",
    "typescript": "^3.5.3",
    "typescript-eslint-language-service": "^1.3.0"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/imagesloaded": "^4.1.1",
    "@types/minimatch": "^3.0.3",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.6.8",
    "@types/puppeteer": "^1.19.0",
    "@types/rimraf": "^2.0.2",
    "@types/yargs": "^13.0.1",
    "chalk": "^2.4.1",
    "imagesloaded": "^4.1.4",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "puppeteer": "^1.19.0",
    "rimraf": "^2.6.2",
    "wait-on": "^3.3.0",
    "yargs": "^13.3.0"
  }
}
